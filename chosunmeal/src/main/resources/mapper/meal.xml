<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="chatbot.chosunmeal.repository.MealMapper">

    <select id="selectmeals" resultType="hashMap">
            select * from meal
    </select>
    <select id="selectmeals_1" resultType="hashMap">
        select menu from meal where day = N'월'
    </select>
    <select id="selectmeals_2" resultType="Map">
        select menu from meal where day = N'화'
    </select>

    <select id="selectmeals_3" resultType="Map">
        select menu from meal where day = N'수'
    </select>

    <select id="selectmeals_4" resultType="Map">
        select menu from meal where day = N'목'
    </select>

    <select id="selectmeals_5" resultType="Map">
        select menu from meal where day = N'금'
    </select>

</mapper>